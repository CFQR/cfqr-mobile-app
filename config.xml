<?xml version='1.0' encoding='utf-8'?>
<widget id="cfqr.questionnaire.app" version="0.17.6" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>CFQR</name>
    <description>CFQ-R questionnaire application</description>
    <author email="cfqr.app@gmail.com" href="https://cfqr.github.io/">CFQR app</author>
    <content src="index.html" />
    <access origin="*" />
    <config-file platform="android" parent="/manifest" mode="merge">
      <application android:debuggable="false" />
    </config-file>
    <uses-permission android:name="android.permission.INTERNET" />
    <icon src="www/icons/256x256.png" />
    <platform name="amazon-fireos">
        <icon density="ldpi" src="www/icons/android-icon-36x36.png" />
        <icon density="mdpi" src="www/icons/android-icon-48x48.png" />
        <icon density="hdpi" src="www/icons/android-icon-72x72.png" />
        <icon density="xhdpi" src="www/icons/android-icon-96x96.png" />
        <icon density="xxhdpi" src="www/icons/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="www/icons/android-icon-192x192.png" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="www/icons/android-icon-36x36.png" />
        <icon density="mdpi" src="www/icons/android-icon-48x48.png" />
        <icon density="hdpi" src="www/icons/android-icon-72x72.png" />
        <icon density="xhdpi" src="www/icons/android-icon-96x96.png" />
        <icon density="xxhdpi" src="www/icons/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="www/icons/android-icon-192x192.png" />
    </platform>
    <platform name="firefoxos">
        <icon height="60" src="www/icons/apple-icon-60x60.png" width="60" />

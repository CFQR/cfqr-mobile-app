        <icon height="57" src="www/icons/apple-icon-57x57.png" width="57" />
        <icon height="114" src="www/icons/apple-icon-114x114.png" width="114" />
        <icon height="72" src="www/icons/apple-icon-72x72.png" width="72" />
        <icon height="144" src="www/icons/apple-icon-144x144.png" width="144" />
